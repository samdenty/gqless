{
  "name": "gqless",
  "version": "2.0.3",
  "description": "gqless client without queries",
  "homepage": "https://gqless.com",
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/gqless.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "build gqless",
    "dev": "nodemon",
    "dev:nodemon": "tsup src -d dist && node dist/QueryBuilder/buildQuery.js",
    "playground": "jest test/playground.test.ts --watch --no-coverage",
    "prepare": "build gqless",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "watch gqless",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/concurrently": "^6.0.1",
    "@types/lodash": "^4.14.168",
    "@types/lodash-es": "^4.17.4",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.35",
    "@types/nodemon": "^1.19.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/tap": "^14.10.3",
    "@types/tsc-watch": "^4.2.0",
    "@types/wait-on": "^5.2.0",
    "@types/ws": "^7.4.0",
    "build-utils": "workspace:*",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "get-port": "^5.1.1",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "mercurius": "^7.3.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.7",
    "open-cli": "^6.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "size-limit": "^4.10.1",
    "tap": "^14.11.0",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.1.0",
    "tsup": "^4.8.19",
    "typescript": "^4.2.3",
    "wait-on": "^5.3.0",
    "ws": "^7.4.4"
  },
  "peerDependencies": {
    "graphql": "^15.5.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "size-limit": [
    {
      "path": "dist/gqless.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqless.esm.js",
      "limit": "20 KB"
    }
  ]
}
