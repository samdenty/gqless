{
  "name": "test-utils",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "src/index.d.ts",
  "bin": {
    "jest-test": "bin/jest-test.js"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs",
    "prepare": "pnpm build",
    "size": "size-limit",
    "start": "nodemon --exec \"concurrently pnpm:build tsc\" -w src/index.ts",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^26.0.22",
    "cross-env": "^7.0.3",
    "esbuild": "^0.9.7",
    "fastify": "^3.14.0",
    "get-port": "^5.1.1",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "mercurius": "^7.3.0",
    "mercurius-codegen": "^1.3.8",
    "mercurius-integration-testing": "^3.1.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "randomstring": "^1.1.5",
    "ts-jest": "^26.5.4",
    "wait-for-expect": "^3.0.2"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/randomstring": "^1.1.6",
    "concurrently": "^6.0.0",
    "rimraf": "^3.0.2",
    "size-limit": "^4.10.1",
    "tslib": "^2.1.0",
    "tsup": "^4.8.19",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/test-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test-utils.esm.js",
      "limit": "10 KB"
    }
  ]
}
